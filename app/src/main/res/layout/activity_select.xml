<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".SelectActivity" android:background="#595959">

    <ImageView android:layout_width="match_parent" android:layout_height="0dp"
               android:layout_weight="2" android:src="@drawable/aistylist_logo"/>

    <ImageView android:id="@+id/imageView" android:layout_width="match_parent" android:layout_height="0dp"
               android:layout_weight="1" android:src="@drawable/ic_baseline_camera_alt_24"/>

    <Button android:id="@+id/uploadBtn" android:layout_width="match_parent" android:layout_height="0dp"
            android:layout_weight="1" android:text="동영상 선택하기"
            android:layout_margin="20dp"/>


</LinearLayout>